{
  "name": "dbmark",
  "version": "1.0.0",
  "description": "database benchmark tool",
  "engines": {
    "node": ">=8"
  },
  "main": "bin",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:cov": "jest && codecov"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "src/.*?__tests__/.*\\.ts$",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "testconfig.js",
      "package.json",
      "package-lock.json"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Soontao/DBMark.git"
  },
  "keywords": [
    "database"
  ],
  "author": "Theo Sun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Soontao/DBMark/issues"
  },
  "homepage": "https://github.com/Soontao/DBMark#readme",
  "devDependencies": {
    "@types/jest": "^24.0.9",
    "@types/node": "^8.0.25",
    "@types/uuid": "^3.4.3",
    "codecov": "^3.2.0",
    "eslint": "^5.15.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "jest": "^24.1.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "4",
    "tslint": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^3.3.3333"
  },
  "dependencies": {
    "faker": "^4.1.0",
    "sqlite3": "^4.0.6",
    "uuid": "^3.3.2"
  }
}